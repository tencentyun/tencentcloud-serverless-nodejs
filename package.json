{
  "name": "tencentcloud-serverless-nodejs",
  "version": "1.0.1",
  "description": "Tencentcloud-serverless SDK for Nodejs",
  "main": "./dist/index.js",
  "repository": "https://github.com/tencentyun/tencentcloud-serverless-nodejs.git",
  "author": "ycp424c",
  "license": "MIT",
  "scripts": {
    "init": "commitizen init cz-conventional-changelog --save --save-exact",
    "build": "tsc --outDir dist/ -p tsconfig.json",
    "tsn": "ts-node --no-cache",
    "release": "standard-version",
    "pub": "git push --follow-tags origin master && npm publish",
    "test": "jest --config jest.config.js",
    "cov": "codecov",
    "lint": "eslint --fix --ext .js,.ts,.tsx ./src"
  },
  "engines": {
    "node": ">= 7.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@types/es6-promise": "^3.3.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.137",
    "@types/node": "^11.13.8",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "codecov": "^3.3.0",
    "commitizen": "^3.1.1",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.15.3",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^24.7.1",
    "prettier": "1.18.2",
    "standard-version": "^4.4.0",
    "typescript": "^3.2.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "keywords": [
    "serverless"
  ],
  "dependencies": {
    "@tencent-sdk/capi": "^1.1.5"
  }
}
